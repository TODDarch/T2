    function queryTicMain() {
        //debugger;
        var tpid = "";
        if ($("#TPID").val() !== "請選擇") {
            tpid = $("#TPID").val()
        }
        else {
            tpid = "";
        }
        var DATE = $("#DATE").data("datepicker").getDate();
        var tempDate = DATE.getFullYear() + "-" + (DATE.getMonth() + 1) + "-" + DATE.getDate();
        var data = {
            "PSID": $("#PSID").val(),
            "TPID": tpid,
            "SQID": $("#SQID").val(),
            "DATE": tempDate,
            "ACID": $("#ACID").val(),
            "SBID": $("#SBID").val(),
            "NOTE": $("#NOTE").val(),
            "JSKD": $("#JSKD").val(),
            "CKID": $("#CKID").val(),
