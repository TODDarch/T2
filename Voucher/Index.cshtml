            "REM1": $("#REM1").val(),
            "REM2": $("#REM2").val(),
            "NIKD": $("#NIKD").val(),
            "CKYM": $("#CKYM").val()
    }   ;

    alert(tempDate);

    jQuery.get('@Url.Action("QueryACHD", "Voucher")', data, 'json')
        .done(function (result) {
            //ac8032-vb
            //alert(result);
            var jObj = JSON.parse(result, function (k, v) {
                if (v) {
                    return v;
                }
                else {
                    return '';
                }
            });
            //alert(jObj);
            $('#ac8032-q table tbody').html('');//CLEAN DATA//
            for (var i = 0; i < jObj.length; i++) {
                var htmlRow = "<tr>";
                htmlRow += "<td>" + "<input type=text name=TPID value=" + jObj[i].TPID + "></td>";
                htmlRow += "<td>" + "<input type=text name=SQID value=" + jObj[i].SQID + "></td>";
                htmlRow += "<td>" + "<input type=text name=DATE value=" + new Date(parseInt((jObj[i].DATE).replace("/Date(", "").replace(")/", ""), 10)).toLocaleString().substr(0, 9) + "></td>";
                htmlRow += "<td>" + "<input type=text name=SBID value=" + jObj[i].SBID + "></td>";
                htmlRow += "<td>" + "<input type=text name=CKID value=" + jObj[i].CKID + "></td>";
                htmlRow += "<td>" + (i + 1).toString() + "</td>";
                htmlRow += "<td>" + "<a href=/Voucher/Index?TPID=" + jObj[i].TPID + "&SQID=" + jObj[i].SQID + ">" + "詳細" + "</a >";
                htmlRow += "</td></tr>";
                $('#ac8032-q table tbody').append(htmlRow);
            }
            $('#ac8032-q input').addClass("noBdrLine");
            //$.each(jObj, function (o) {
            //    alert(o.PSID);
            //})
